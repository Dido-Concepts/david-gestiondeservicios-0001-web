{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "eslint-config-standard"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "@next/next/no-img-element": "off"
  },
  "overrides": [
    {
      "files": ["*.entity.ts", "*.model.ts"],
      "rules": {
        "no-use-before-define": "off"
      }
    }
  ]
}